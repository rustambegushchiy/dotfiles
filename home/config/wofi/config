# config файл wofi
# от Begushchiy

# Стиль находиться по пути ~/.config/wofi/style.css

#===== КОНФИГ =================================================================#

# Скрытие ползунка скрола
hide_scroll=true

# Отключение лишних опций
no_actions=true

# Режим
show=drun

# Надпись в строке поиска
prompt=Search:

# Терминал
term=kitty

# Отключение иконок
allow_images = false

